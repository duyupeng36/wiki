{
    "root":".",
    "author":"杜宇鹏",
    "title":"计算机科学与技术",
    "language":"zh-hans",
    "description":"计算机科学与技术专业课程",
    "isbn":"000-0-00-000000-0",
    "direction":"ltr",
    "gitbook":">=3.2.3",
    "structure":{
        "readme":"README.md",
        "summary":"SUMMARY.md",
        "glossary":"GLOSSARY.md"
    },
    "variables": {
        "github": "https://github.com/duyupeng36"
    },
    "links":{
        "sidebar":{
            "CSND":"https://blog.csdn.net/weixin_42470629",
            "Github":"https://github.com/duyupeng36"
        }
    },
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "pdf":{
        "pageNumbers":true,
        "fontFamily":"Arial",
        "fontSize":12,
        "paperSize":"a4",
        "margin":{
            "right":62,
            "left":62,
            "top":56,
            "bottom":56
        }
    },
    "plugins":[
        "splitter",
        "code",
        "advanced-emoji",
        "expandable-chapters",
        "alerts",
        "flexible-alerts",
        "popup",
        "tbfed-pagefooter",
        "hide-element",
        "back-to-top-button",
        "disqus",
        "-sharing", "sharing-plus",
        "github",
        "chapter-fold",
        "sidebar-style",
        "katex",
        "simple-page-toc",
        "page-toc-button",
        "-lunr", "-search", "search-plus",
        "-change_girls",
        "pageview-count",
        "prism",
        "-highlight",
        "prism-themes"
    ],
    "pluginsConfig": {
        "tbfed-pagefooter": {
            "copyright":"Copyright @duyupeng 2025-2025",
            "modify_label": "更新时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "disqus": {
            "shortName": "wiki"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "twitter": false,
            "weibo": false,
            "all": [
                "douban", "facebook", "google", "instapaper", "linkedin","twitter", "weibo",
                "messenger","qq", "qzone","viber","whatsapp"
            ]
        },
        "github": {
            "url": "https://github.com/duyupeng36"
        },
        "sidebar-style": {
            "title": "计算机科学与技术",
            "author": "杜宇鹏"
        },
        "flexible-alerts": {
            "style": "callout",
            "question": {
                "label": "Question",
                "icon": "fa fa-question",
                "className": "warning"
            }
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "change_girls" : {
            "time" : 10,
            "urls" : [
                "http://cdn.jsdelivr.net/gh/duyupeng36/images@master/pictures/1755692806631-dd5baa4b95bc4ecc919603562064e8ca.jpg", 
                "http://cdn.jsdelivr.net/gh/duyupeng36/images@master/pictures/1755692806632-ebb99e9e166f4e419335ffef05698360.jpg",
                "http://cdn.jsdelivr.net/gh/duyupeng36/images@master/pictures/1755692806643-a9230c8ca5fc482381ad9368626c9a81.jpeg",
                "http://cdn.jsdelivr.net/gh/duyupeng36/images@master/pictures/1755883969636-9fa3bf96ac704d52821b8b90e24eb054.jpg"
            ]
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ],
            "lang": {
                "flow": "typescript"
            },
            "ignore": [
                "mermaid",
                "eval-js"
            ]
        }
    }
}
